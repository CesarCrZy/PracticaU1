<%include _header%>
    <h1>Users</h1>

    <%users.forEach((equip) =>{%>
        <div style="" class="panel panel-info">
            <div style="background-color: #545357" class="panel-heading">
                <a href="/users1/<%=equip.username%>">
                    <%=equip.name()%>
                </a>
            </div>
            <%if (equip.role){%>
                <div style="color: black;" class="panel-body">
                    <%=equip.role%>
                    
                </div>

            <%}%>
        </div>
        
        
        <%})%>

            <%include _footer%>